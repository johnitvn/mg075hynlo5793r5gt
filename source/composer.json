{
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-jui": "~2.0.0",
        "facebook/php-sdk-v4": "~5.0",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-widgets": "*",
        "dmstr/yii2-migrate-command": "*",
        "bower-asset/PACE": "1.0.2",
        "bower-asset/slimScroll": "1.3.6",
        "bower-asset/metisMenu": "2.1.0",
        "bower-asset/iCheck": "1.0.2",
        "bower-asset/toastr": "2.1.1",
        "bower-asset/sweetalert": "1.1.0",
        "bower-asset/bootstrap-datepicker": "1.4.0",
        "bower-asset/cropper": "0.11.1",
        "bower-asset/chosen": "1.4.2",
        "bower-asset/animate.css": "*",
        "bower-asset/font-awesome": "*"
    },
    "require-dev": {
        "yiisoft/yii2-apidoc": "~2.0.0",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "slim/slim": "2.*",
        "slim/views": "0.1.*",
        "twig/twig": "~1.13",
        "symfony/yaml": "2.5.*@dev",
        "yiisoft/yii2-codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
    }
}
